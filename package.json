{
  "name": "vuejs-slack-clone",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev:server": "nodemon ./bin/www",
    "dev:client": "cd client && npm run serve",
    "dev": "concurrently --kill-others-on-fail 'npm run dev:server' 'npm run dev:client'",
    "start": "node ./bin/www"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.18.2",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.2"
  }
}
